<?xml version="1.0" encoding="utf-8"?><xsl:transform version="1.0" id="ViewNLM-v2-04_scielo.xsl" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:util="http://dtd.nlm.nih.gov/xsl/util" xmlns:mml="http://www.w3.org/1998/Math/MathML" exclude-result-prefixes="util xsl">	<xsl:template match="*[contains(front/journal-meta/issn,'1678-4685') or contains(front/journal-meta/issn,'1415-4757')]//citation/year" mode="none">		(<xsl:apply-templates/>)	</xsl:template>		<xsl:template match="*[contains(front/journal-meta/issn,'1678-4685') or contains(front/journal-meta/issn,'1415-4757')]//citation//name">		<xsl:value-of select="surname"/>		<xsl:text> </xsl:text>		<xsl:apply-templates select="given-names"/>		<xsl:choose>			<xsl:when test="position()=last()"></xsl:when>			<xsl:when test="position()=last()-1"> and </xsl:when>			<xsl:otherwise>, </xsl:otherwise>		</xsl:choose>	</xsl:template>	<xsl:template match="*[contains(front/journal-meta/issn,'1678-4685') or contains(front/journal-meta/issn,'1415-4757')]//citation//given-names">		<xsl:value-of select="translate(.,'.','')"/>	</xsl:template>	<xsl:template match="*[contains(front/journal-meta/issn,'1678-4685') or contains(front/journal-meta/issn,'1415-4757')]//citation/article-title" mode="none">		<xsl:apply-templates select="."/><xsl:apply-templates select="../subtile"/>.	</xsl:template>	<xsl:template match="*[contains(front/journal-meta/issn,'1678-4685') or contains(front/journal-meta/issn,'1415-4757')]//citation/subtitle">. <xsl:apply-templates select="."/>	</xsl:template>	<xsl:template match="*[contains(front/journal-meta/issn,'1678-4685') or contains(front/journal-meta/issn,'1415-4757')]//citation[@citation-type='book' or @citation-type='other']/publisher-name" mode="none"></xsl:template>	<xsl:template match="*[contains(front/journal-meta/issn,'1678-4685') or contains(front/journal-meta/issn,'1415-4757')]//citation[@citation-type='book' or @citation-type='other']/publisher-loc" mode="none"><xsl:apply-templates select="../publisher-name"/>, <xsl:apply-templates select="."/>, 	</xsl:template>	<xsl:template match="*[contains(front/journal-meta/issn,'1678-4685') or contains(front/journal-meta/issn,'1415-4757')]//citation[@citation-type='thesis']/publisher-name" mode="none"></xsl:template>	<xsl:template match="*[contains(front/journal-meta/issn,'1678-4685') or contains(front/journal-meta/issn,'1415-4757')]//citation[@citation-type='thesis']/publisher-loc" mode="none"><xsl:apply-templates select="../publisher-name"/>, <xsl:apply-templates select="."/>.	</xsl:template>	<xsl:template match="*[contains(front/journal-meta/issn,'1678-4685') or contains(front/journal-meta/issn,'1415-4757')]//citation[@citation-type='thesis']/source" mode="none">		<xsl:choose>			<xsl:when test="contains(.,', Master')">				<xsl:value-of select="substring-before(.,', Master')"/>. Master<xsl:value-of select="substring-after(.,', Master')"/>			</xsl:when>			<xsl:when test="contains(.,', Doctor')">				<xsl:value-of select="substring-before(.,', Doctor')"/>. Doctor<xsl:value-of select="substring-after(.,', Doctor')"/>			</xsl:when>			<xsl:when test="contains(.,', PhD Thesis')">				<xsl:value-of select="substring-before(.,', PhD Thesis')"/>. PhD Thesis<xsl:value-of select="substring-after(.,', PhD Thesis')"/>			</xsl:when>			<xsl:otherwise><xsl:apply-templates/></xsl:otherwise>		</xsl:choose>,	</xsl:template>	<xsl:template match="*[@citation-type='book' and ./source and ./article-title]/person-group[@person-group-type='editor']" mode="none">			</xsl:template>	<xsl:template match="*[@citation-type='book' and article-title and person-group[@person-group-type='editor']]/source" mode="none">				In: <xsl:apply-templates select="..//person-group[@person-group-type='editor']//name"/> (eds)		<xsl:apply-templates select="."/><xsl:apply-templates select="../volume"/>.	</xsl:template>	<xsl:template match="*[@citation-type='book' and ./source and ./article-title]/fpage" mode="none"> pp <xsl:apply-templates select="."/>-<xsl:apply-templates select="../lpage"/><xsl:apply-templates select="../comment" mode="text"/>.	</xsl:template>	<xsl:template match="*[@citation-type='journal' and comment]/fpage" mode="none">:<xsl:apply-templates select="."/>-<xsl:apply-templates select="../lpage"/><xsl:apply-templates select="../comment" mode="text"/>.	</xsl:template>	<xsl:template match="*[@citation-type]/comment" mode="text"> (<xsl:value-of  select="."/>)	</xsl:template>	<xsl:template match="*[@citation-type]/comment" mode="none"><xsl:choose>		<xsl:when test="*">			<xsl:apply-templates/>		</xsl:when>		<xsl:otherwise></xsl:otherwise>	</xsl:choose></xsl:template>	<xsl:template match="*[@citation-type='book']/volume" mode="none"></xsl:template>	<xsl:template match="*[@citation-type='book']/volume">, v. <xsl:value-of  select="."/>		</xsl:template>	<xsl:template match="*[@citation-type='book']//edition" mode="none"><xsl:value-of select="."/>. </xsl:template></xsl:transform>